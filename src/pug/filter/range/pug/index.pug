.container.filter
	ul.range-wrapper
		each v in ['Стоимость:', 'Этаж:', 'Площадь:']
			li.range-item
				h4.range-item__label= v
				input#val_price.js-range-init(type="hidden" data-from="1" data-min="1" data-to="30000000" data-max="30000000")
				.u-range-item-nav
					input#price_min.js_val_price_min.slider__currentMin(name="price_min" type="number" min="1" value="1" max="30000000")
					input#price_max.js_val_price_max.slider__currentMax(name="price_max" type="number" min="1" value="30000000" max="30000000")


